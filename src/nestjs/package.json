{
  "name": "@config-service/nestjs",
  "version": "0.0.0-alpha.2",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "pnpm watch",
    "prebuild": "rm -rf tsconfig.build.tsbuildinfo ./dist",
