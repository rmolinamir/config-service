    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@config/tsconfig": "workspace:*",
    "@config-service/core": "workspace:*",
    "@faker-js/faker": "7.5.0",
    "@nestjs/common": "~9.1.4",
    "@nestjs/core": "~9.1.4",
    "@nestjs/testing": "~9.1.4",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.3",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.13.2",
    "jest": "~29.1.2",
    "reflect-metadata": "~0.1.13",
    "release-it": "~15.5.0",
    "ts-jest": "~29.0.3",
    "typescript": "~4.8.4"
  }
