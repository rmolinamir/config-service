    "test:watch": "pnpm jest --watch",
    "watch": "tsc --project tsconfig.build.json --watch"
  },
  "peerDependencies": {
    "@config-service/core": "0.0.0-alpha.1",
    "@nestjs/common": "^8.0.0 || ^9.0.0",
