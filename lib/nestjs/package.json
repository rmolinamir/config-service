{
  "name": "@config-service/nestjs",
  "version": "0.0.0-alpha.3",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "pnpm watch",
    "lint:fix": "tsc --noEmit && eslint --fix",
    "lint:prepublish": "tsc --project tsconfig.build.json --noEmit && eslint",
    "lint": "tsc --noEmit && eslint",
    "prebuild": "rm -rf tsconfig.build.tsbuildinfo ./dist",
    "prepublish": "pnpm build",
    "release:dry": "pnpm release-it --dry-run",
    "release": "pnpm release-it",
    "test:watch": "pnpm jest --watch",
    "test": "pnpm jest",
    "watch": "tsc --project tsconfig.build.json --watch"
  },
  "peerDependencies": {
    "@config-service/core": "^0.0.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@config/tsconfig": "workspace:*",
    "@config-service/core": "^0.0.0",
    "@faker-js/faker": "7.6.0",
    "@nestjs/common": "~9.4.0",
    "@nestjs/testing": "~9.4.0",
    "@types/jest": "29.5.1",
    "@types/node": "20.1.0",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "jest": "~29.5.0",
    "reflect-metadata": "~0.1.13",
    "release-it": "~15.10.3",
    "ts-jest": "~29.1.0",
    "typescript": "~5.0.4"
  }
}
