{
  "name": "@config-service/testing",
  "version": "0.0.0",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "files": ["dist"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint:fix": "tsc --noEmit && eslint --fix",
    "lint:prepublish": "tsc --project tsconfig.build.json --noEmit && eslint",
    "lint": "tsc --noEmit && eslint",
    "prebuild": "rm -rf tsconfig.build.tsbuildinfo ./dist",
    "prepublish": "pnpm build",
    "release:dry": "pnpm release-it --dry-run",
    "release": "pnpm release-it",
    "test:watch": "pnpm jest --watch",
    "test": "pnpm jest",
    "watch": "tsc --project tsconfig.build.json --watch"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@config/jest": "workspace:*",
    "@config/tsconfig": "workspace:*",
    "@faker-js/faker": "~8.4.1",
    "@types/jest": "~29.5.12",
    "@types/node": "~20.14.10",
    "jest": "~29.7.0",
    "release-it": "~17.5.0",
    "ts-jest": "~29.2.0",
    "typescript": "~5.5.3"
  }
}
